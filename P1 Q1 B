#include <stdio.h>

int main() {
    int n, contador:
    printf("Digite um valor inteiro positivo:"):
    scanf("%i", &n);

    if(n < 1) {
        printf("ERROR");
        return 0;
    }

    printf("Numeros primos menores que %i;\n",n);

    for(int i = 2; i < n; i++) {
        contador = 0;

        for(int j = 1; j <= i, j++) {
            if(i % j == 0) {
                contador ++;
            }
        }

        if(contador == 2) {
            printf("%i",i)
        }
    }

    printf("\n");
    return 0;
}
