#include <stdio.h>

int main() {
    int n, i , is_prime = 1;

    printf("Digite um numero inteiro positivo");
    scanf("%d",&n);

    if(n <= 1) {
        printf("O numero não é primo. \n");
        return 0;
    }

    for(i = 2;i*i <= n; i++) {
        if(n % 1 == 0) {
            is_prime = 0;
        }
    }

}
